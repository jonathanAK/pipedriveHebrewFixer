{
  "name": "Pipedrive hebrew fixer",
  "description": "makes hebrew not horrible to read.      Icons made by https://www.flaticon.com/authors/freepik",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://webiks.pipedrive.com/*"
      ],
      "css": [
        "background.css"
      ]
    }
  ],
  "action": {
    "default_icon": "icons/icon128.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  }
}
